{
  "name": "@pyro-labs/core",
  "version": "0.0.1",
  "description": "Core types and interfaces for Pyro SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "pyro",
    "sdk",
    "blockchain",
    "solana",
    "evm",
    "ethereum",
    "polygon",
    "mumbai",
    "arbitrum",
    "optimism",
    "base",
    "sepolia",
    "holesky",
    "goerli",
    "mainnet",
    "testnet",
    "devnet",
    "network",
    "config",
    "rpc",
    "url",
    "chain",
    "id",
    "chainId",
    "types",
    "interfaces"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DGrayArea/pyro-workspace.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist"
  ]
}
