{
  "name": "@pyro-labs/unoswap",
  "version": "0.0.1",
  "description": "Unoswap quoter for aggregating quotes across DEX versions and fee tiers",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "pyro",
    "sdk",
    "blockchain",
    "uniswap",
    "dex",
    "quoter",
    "swap"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DGrayArea/pyro-workspace.git",
    "directory": "packages/unoswap"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@pyro-labs/core": "workspace:*",
    "@pyro-labs/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist"
  ]
}
